{
  "enabled": true,
  "ansible-galaxy": {
    "enabled": true
  },
  "packageRules": [
    {
      //"Pin ansible galaxy haxorof.docker_ce to version supported on centos7",
      "matchDatasources": ["galaxy"],
      "allowedVersions": "<3.3",
      "matchPackageNames": [
        "haxorof.docker_ce"
      ]
    }
  ]
}