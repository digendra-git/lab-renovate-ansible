{
  "enabled": true,
  "ansible-galaxy": {
    "enabled": true
  },
  "packageRules": [
    {
      // Keep ansible haxorof.docker_ce galaxy to a version supporting centos7
      "matchDatasources": ["galaxy"],
      "allowedVersions": "<3.3",
      "matchPackageNames": [
        "haxorof.docker_ce"
      ]
    }
  ]
}